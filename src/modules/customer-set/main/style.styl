.container
  position relative
  .slot
    height: 56px
    background: #FFFFFF
    position: absolute
    bottom: 0
    width: 100%;
    left: 0;
    right: 0;
    border-radius: 4px
  .operate
    height: 36px
    position: fixed
    bottom: 40px
    right: 36px
    z-index 10
